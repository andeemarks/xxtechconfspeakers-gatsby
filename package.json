{
  "name": "xxtechconfspeakers",
  "version": "1.0.0",
  "description": "Website for www.xxtechconfspeakers.org",
  "author": "Andy Marks <vampwillow@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/andeemarks/xxtechconfspeakers-gatsby"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andeemarks/xxtechconfspeakers-gatsby/issues"
  },
  "homepage": "https://github.com/andeemarks/xxtechconfspeakers-gatsby",
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "next",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-typescript": "^2.1.18",
    "gatsby-plugin-typography": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-json": "next",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "react-vis": "^1.11.7",
    "time-ago": "^0.2.1",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19",
    "underscore": "^1.9.1"
  },
  "resolutions": {
    "set-value": "^2.0.1",
    "mixin-deep": "^1.3.2"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "ajv": "ajv validate -s ./spec/confs-schema.json -d ./src/data/confs.json",
    "test": "nyc ava",
    "check-urls": "node ./spec/UrlChecker.js",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js' 'spec/**/*.js'",
    "type-check": "tsc"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "ajv-cli": "~3.0.0",
    "ava": "^2.4.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "gatsby-cli": "next",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "mockdate": "^2.0.5",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "exclude": [
      "spec"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn format && yarn lint && yarn ajv"
    }
  }
}
