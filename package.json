{
  "name": "xxtechconfspeakers",
  "version": "1.0.0",
  "description": "Website for www.xxtechconfspeakers.org",
  "author": "Andy Marks <vampwillow@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/andeemarks/xxtechconfspeakers-gatsby"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andeemarks/xxtechconfspeakers-gatsby/issues"
  },
  "homepage": "https://github.com/andeemarks/xxtechconfspeakers-gatsby",
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "next",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-typography": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-json": "next",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "react-vis": "^1.11.4",
    "time-ago": "^0.2.1",
    "typography": "^0.16.17",
    "typography-theme-bootstrap": "^0.16.7",
    "underscore": "^1.9.1"
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "ajv": "ajv validate -s ./spec/confs-schema.json -d ./src/data/confs.json",
    "test": "nyc jasmine",
    "check-urls": "node ./spec/UrlChecker.js",
    "precommit": "yarn test && yarn format && yarn lint",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'"
  },
  "devDependencies": {
    "ajv-cli": "~3.0.0",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "gatsby-cli": "next",
    "jasmine": "^3.3.0",
    "jasmine-collection-matchers": "^0.2.0",
    "mockdate": "^2.0.2",
    "nyc": "^13.1.0",
    "prettier": "^1.15.2"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "exclude": [
      "spec"
    ]
  }
}
