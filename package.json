{
  "name": "xxtechconfspeakers",
  "version": "1.0.0",
  "description": "Website for www.xxtechconfspeakers.org",
  "author": "Andy Marks <vampwillow@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/andeemarks/xxtechconfspeakers-gatsby"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andeemarks/xxtechconfspeakers-gatsby/issues"
  },
  "homepage": "https://github.com/andeemarks/xxtechconfspeakers-gatsby",
  "dependencies": {
    "@types/numeral": "^0.0.28",
    "@types/underscore": "^1.11.2",
    "classnames": "^2.3.1",
    "gatsby": "^2.32.13",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-typescript": "^2.12.1",
    "gatsby-plugin-typography": "^2.12.0",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-json": "^2.11.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.19",
    "react-vis": "^1.11.7",
    "time-ago": "^0.2.1",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19",
    "underscore": "^1.13.1"
  },
  "resolutions": {
    "set-value": "^2.0.1",
    "mixin-deep": "^1.3.2"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "yarn lint --fix",
    "ajv": "ajv validate -s ./spec/confs-schema.json -d ./src/data/confs.json",
    "test": "nyc ava",
    "check-urls": "node ./spec/UrlChecker.js",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js' 'spec/**/*.js'",
    "type-check": "tsc"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "ajv-cli": "~3.3.0",
    "ava": "^3.15.0",
    "commitizen": "^4.2.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "gatsby-cli": "^2.19.2",
    "husky": "^8.0.2",
    "lint-staged": "^10.5.4",
    "mockdate": "^3.0.5",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "source-map-support": "^0.5.16",
    "ts-node": "^9.1.1",
    "typescript": "^4.9.5"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "exclude": [
      "spec"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn format && yarn lint && yarn ajv",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
